<% if current_user %>
<%= button_to "Refresh", root_path, :method => :get %>
  <h1>User Emails</h1>
  <table>
    <%= render :partial => 'email', :collection => @emails%>
  </table>
  <br />
  <%= will_paginate @emails %>
</div>
  <p><%= link_to "Log out", logout_path %></p>
<% else %>
  <h1>TestProject</h1>
  <p><strong><%=t :sign_in %></strong></p>
    <a href="/auth/google_oauth2" class="auth_provider">
      <%= image_tag "google_apps_64.png", size: "64x64", alt: "Google" %>
      Google
    </a>
  </div>
 <% end %>
